{
  "name": "DateFIle",
  "version": "0.1.0",
  "description": "index files in a directory using the date of another file as a filter",
  "main": "./main/main.js",
  "scripts": {
    "start": "concurrently \"npm run react-start\" \"npm run electron-start\"",
    "react-start": "react-scripts start",
    "electron-start": "electron .",
    "webpack-electron": "npx webpack --config webpack.electron.config.js --mode=production",
    "webpack-react": "npx webpack --config webpack.react.config.js --mode=production",
    "webpack": "concurrently \"npm run webpack-react\" \"npm run webpack-electron\"",
    "build-win": "electron-packager . DateFile --platform=win32 --arch=x64 --mode=production --out=./.bin --ignore=node_modules --ignore=mainDev.js --ignore=electron-builder.json --ingore=webpack.config.js --ingore=webpack.react.config.js  --ingore=webpack.electron.config.js --icon=favicon.ico --app-version=0.1.0 --asar",
    "build-macos": "electron-packager . DateFile --platform=darwin --arch=x64 --mode=production --out=./.bin --ignore=node_modules --ignore=mainDev.js --ignore=electron-builder.json --ingore=webpack.config.js --ingore=webpack.react.config.js  --ingore=webpack.electron.config.js --icon=./assets/datefile.png --app-version=0.1.0 --asar",
    "build-linux": "electron-packager . DateFile --platform=linux --arch=x64 --mode=production --out=./.bin --ignore=node_modules --ignore=mainDev.js --ignore=electron-builder.json --ingore=webpack.config.js --ingore=webpack.react.config.js  --ingore=webpack.electron.config.js --icon=./assets/datefile.png --app-version=0.1.0 --asar",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "file",
    "index",
    "search",
    "dates",
    "directory",
    "electron",
    "nodejs",
    "reactjs"
  ],
  "author": "Kirolos A.",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.22.5",
    "@babel/preset-env": "^7.22.5",
    "@babel/preset-react": "^7.22.5",
    "babel-loader": "^9.1.2",
    "concurrently": "^8.2.0",
    "electron": "^25.0.1",
    "electron-packager": "^17.1.1",
    "react-scripts": "5.0.1",
    "webpack": "^5.86.0",
    "webpack-cli": "^5.1.4"
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.4",
    "date-fns": "^2.30.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-virtualized-auto-sizer": "^1.0.20",
    "react-window": "^1.8.9"
  },
  "browserslist": {
    "production": [
      "electron >= 25.0.1"
    ],
    "development": [
      "electron >= 25.0.1"
    ]
  }
}
